{
	"name": "quykshop-master-api",
	"version": "1.0.0",
	"description": "authentication and authorization server for quykshop",
	"repository": "https://gitlab.com/everexpert/quykshop-master-api",
	"author": "Mamun Bhuiyan <mamunbhuiyan919@gmail.com>",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"seed:dev": "ts-node -r tsconfig-paths/register src/seed.ts",
		"seed-category:dev": "ts-node -r tsconfig-paths/register src/seed_category.ts",
		"seed:prod": "node dist/src/seed.js",
		"typeorm": "ts-node node_modules/typeorm/cli.js  -f ormconfig",
		"migration:generate": "yarn run typeorm migration:generate -d src/migrations -n",
		"migration:create": "yarn run typeorm migration:create -d src/migrations -n",
		"migration:revert": "yarn run typeorm migration:revert",
		"schema:drop": "yarn run typeorm schema:drop",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/common": "^7.6.15",
		"@nestjs/config": "^0.6.3",
		"@nestjs/core": "^7.6.15",
		"@nestjs/jwt": "^7.2.0",
		"@nestjs/passport": "^7.1.5",
		"@nestjs/platform-express": "^7.6.15",
		"@nestjs/swagger": "^4.8.0",
		"@nestjs/typeorm": "^7.1.5",
		"bcrypt": "^5.0.1",
		"cache-manager": "^3.4.4",
		"cache-manager-redis-store": "^2.0.0",
		"class-transformer": "^0.4.0",
		"class-validator": "^0.13.1",
		"compression": "^1.7.4",
		"dotenv": "^10.0.0",
		"express-rate-limit": "^5.2.6",
		"helmet": "^4.6.0",
		"lodash": "^4.17.21",
		"morgan": "^1.10.0",
		"mysql2": "^2.2.5",
		"nodemailer": "^6.6.3",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"redis": "^3.1.2",
		"reflect-metadata": "^0.1.13",
		"request-context": "^2.0.0",
		"rimraf": "^3.0.2",
		"rxjs": "^6.6.6",
		"sharp": "^0.28.3",
		"swagger-ui-express": "^4.1.6",
		"typeorm": "^0.2.34",
		"typeorm-transactional-cls-hooked": "^0.1.21"
	},
	"devDependencies": {
		"@nestjs/cli": "^7.6.0",
		"@nestjs/schematics": "^7.3.0",
		"@nestjs/testing": "^7.6.15",
		"@types/bcrypt": "^5.0.0",
		"@types/cache-manager": "^3.4.2",
		"@types/compression": "^1.7.0",
		"@types/express": "^4.17.11",
		"@types/express-rate-limit": "^5.1.2",
		"@types/helmet": "^4.0.0",
		"@types/jest": "^26.0.22",
		"@types/lodash": "^4.14.171",
		"@types/morgan": "^1.9.2",
		"@types/multer": "^1.4.6",
		"@types/node": "^14.14.36",
		"@types/passport-jwt": "^3.0.5",
		"@types/passport-local": "^1.0.33",
		"@types/sharp": "^0.28.4",
		"@types/shelljs": "^0.8.8",
		"@types/supertest": "^2.0.10",
		"@types/webpack-env": "^1.16.0",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"clean-webpack-plugin": "^4.0.0-alpha.0",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"jest": "^26.6.3",
		"prettier": "^2.2.1",
		"supertest": "^6.1.3",
		"ts-jest": "^26.5.4",
		"ts-loader": "^8.0.18",
		"ts-node": "^9.1.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.2.3",
		"webpack": "^5.39.1",
		"webpack-cli": "^4.7.2",
		"webpack-node-externals": "^3.0.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
